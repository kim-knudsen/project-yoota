{
    "name": "mobile",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "tsc --noEmit --pretty && yarn lint",
        "clean": "rimraf .turbo",
        "android": "react-native run-android",
        "android:clean": "rimraf 'android/build' && (cd android; ./gradlew clean)",
        "ios": "react-native run-ios",
        "start": "react-native start",
        "start:clear": "yarn start --reset-cache",
        "xcode": "xed ios",
        "xcode:clean": "cd ios; xcodebuild clean",
        "pod:install": "cd ios; pod install --repo-update",
        "check:dep": "rnx-dep-check",
        "studio": "studio android",
        "studio:clean": "cd android; ./gradlew clean",
        "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx --max-warnings=0",
        "test": "tsc --noEmit && yarn lint && jest -i --config ./jest/unit/jest.config.ts",
        "list:devices:ios": "xcrun simctl list devicetypes",
        "list:devices:android": "emulator -list-avds",
        "build:detox:ios:debug": "detox build --configuration ios.sim.debug",
        "build:detox:android:debug": "detox build --configuration android.emu.debug",
        "test:detox:ios:debug": "detox test --configuration ios.sim.debug --cleanup",
        "test:detox:android:debug": "detox test --configuration android.emu.debug --cleanup"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.3.0",
        "@fortawesome/free-brands-svg-icons": "^6.3.0",
        "@fortawesome/free-regular-svg-icons": "^6.3.0",
        "@fortawesome/free-solid-svg-icons": "^6.3.0",
        "@fortawesome/react-native-fontawesome": "^0.3.0",
        "@react-native-async-storage/async-storage": "^1.17.11",
        "@react-navigation/native": "^6.0.14",
        "@react-navigation/native-stack": "^6.9.2",
        "@tanstack/react-query": "^4.24.10",
        "@trpc/client": "^10.12.0",
        "@yoota/client": "*",
        "@yoota/common": "*",
        "@yoota/i18n": "*",
        "@yoota/logger": "*",
        "@yoota/theme": "*",
        "rambda": "7.4.0",
        "react": "18.2.0",
        "react-native": "0.71.8",
        "react-native-calendars": "1.1289.0",
        "react-native-config": "^1.4.11",
        "react-native-haptic-feedback": "^1.14.0",
        "react-native-inappbrowser-reborn": "^3.7.0",
        "react-native-launch-arguments": "^3.1.2",
        "react-native-linear-gradient": "2.6.2",
        "react-native-localize": "^2.2.4",
        "react-native-reanimated": "^2.14.4",
        "react-native-safe-area-context": "^4.3.3",
        "react-native-screens": "^3.17.0",
        "react-native-svg": "^13.8.0",
        "react-native-url-polyfill": "^1.3.0",
        "react-native-webview": "^11.23.1",
        "superjson": "^1.10.0"
    },
    "devDependencies": {
        "@babel/core": "^7.20.2",
        "@babel/preset-env": "^7.20.0",
        "@babel/runtime": "^7.20.1",
        "@react-native-community/eslint-config": "^3.2.0",
        "@rnx-kit/babel-preset-metro-react-native": "^1.1.3",
        "@rnx-kit/dep-check": "^1.14.0",
        "@rnx-kit/jest-preset": "^0.1.13",
        "@rnx-kit/metro-config": "^1.3.3",
        "@tsconfig/react-native": "^2.0.2",
        "@types/detox": "^18.1.0",
        "@types/jest": "^29.2.3",
        "@types/react": "^18.0.24",
        "@types/react-test-renderer": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.46.1",
        "@typescript-eslint/parser": "^5.46.1",
        "@yoota/eslint-config": "*",
        "babel-jest": "^29.3.1",
        "detox": "^20.0.3",
        "eslint": "8.35.0",
        "jest": "^29.4.3",
        "react-native-svg-transformer": "^1.0.0",
        "react-test-renderer": "18.2.0",
        "typescript": "^5.0.2"
    },
    "jest": {
        "preset": "react-native",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    }
}
